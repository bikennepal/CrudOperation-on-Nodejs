<table class="table-table-striped">
<thead>
    <tr>
        <th>Full name</th>
        <th>Email</th>
        <th>mobile</th>
        <th>city</th>
        <th></th>
    </tr>
</thead>
<tbody>
    {{#each list}}
    <tr>
        <td>{{this.fullname}}</td>
        <td>{{this.email}}</td>
        <td>{{this.mobile}}</td>
        <td>{{this.city}}</td>

        <td>
            <a><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
            <a><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
        </td>
    </tr>
    {{/each}}
</tbody>
</table>